<P>Books#index</P>

<%= render 'users/list', user: @user, book: @book %>

<h2>Books</h2>
<% @books.each do |book| %>

<%= image_tag book.user.get_profile_image(100,100) %>
<p>ユーザーネーム：<%= book.user.name %></p>
<p>タイトル</p>
<%= link_to "Title", book_path(book.id) %>
<span><%= book.title %></span>
<span><%= book.body %></span>

<% end %>